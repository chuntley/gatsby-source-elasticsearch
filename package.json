{
  "name": "gatsby-source-elasticsearch",
  "version": "0.1.0",
  "description": "Gatsby Elasticsearch source plugin",
  "scripts": {
    "build": "babel src --out-dir . --ignore *.test.js",
    "test": "mocha --require babel-core/register ./src/*.test.js",
    "test:watch": "mocha --watch --require babel-core/register ./src/index.test.js",
    "preversion": "yarn test",
    "version": "yarn build"
  },
  "homepage": "https://github.com/chuntley/gatsby-source-elasticsearch",
  "repository": {
    "type": "git",
    "url": "https://github.com/chuntley/gatsby-source-elasticsearch.git"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "elasticsearch"
  ],
  "author": "Chad Huntley",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.2",
    "eslint-plugin-import": "^2.7.0",
    "mocha": "^5.2.0"
  },
  "dependencies": {
    "elasticsearch": "^15.0.0"
  }
}
